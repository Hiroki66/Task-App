<%= form_with(url: user_tasks_path, local: true) do |f| %>
  <!--%= render 'shared/error_messages', object: @task %>-->

  <%= f.label :task, class: "label-form" %>
  <%= f.text_field :task, class: "form-control" %>

  <%= f.label :detail, class: "label-form" %>
  <%= f.text_field :detail, class: "form-control" %>
  
  <%= f.submit "新規登録", class: "btn btn-primary btn-block btn-form" %>

<% end %>