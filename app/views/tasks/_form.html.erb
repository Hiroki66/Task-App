<%= form_with(model: [@user, @task], url: edit_user_task_path, local: true, method: :patch) do |f| %>
  <!--%= render 'shared/error_messages', object: @task %>-->

  <%= f.label :name, class: "label-form" %>
  <%= f.text_field :name, class: "form-control" %>

  <%= f.label :email, class: "label-form" %>
  <%= f.text_field :email, class: "form-control" %>
  
  <%= f.submit "登録", class: "btn btn-primary btn-block btn-form" %>
<% end %>